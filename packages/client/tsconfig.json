{
	"compilerOptions": {
		"outDir": "./dist/",
		"sourceMap": true,
		"noImplicitAny": true,
		"strictNullChecks": true,
		"module": "es6",
		"moduleResolution": "node",
		"target": "es5",
		"jsx": "react",
		"allowSyntheticDefaultImports": true,
		"esModuleInterop": true
	},
	"include": ["./src"]
}
