{
  "name": "@frontend-assignment/client",
  "version": "1.0.0",
  "main": "index.tsx",
  "scripts": {
    "start": "webpack-dev-server --mode development --hot",
    "build": "webpack --mode production",
    "downloadSchema": "apollo schema:download src/graphql/schema.json --endpoint=http://localhost:4000/graphql",
    "generate": "yarn downloadSchema && apollo codegen:generate --target=typescript --excludes=node_modules/* --includes=src/**/*.graphql --endpoint=http://localhost:4000/graphql --tagName=gql --outputFlat=src/@types/generated --localSchemaFile=src/graphql/schema.json && echo ************Remember to remove nulls from the generated types or this won't work...*******************"
  },
  "private": true,
  "devDependencies": {
    "@babel/core": "^7.10.4",
    "@babel/plugin-transform-async-to-generator": "^7.10.4",
    "@babel/plugin-transform-runtime": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-react": "^7.10.4",
    "@types/graphql": "^14.5.0",
    "@types/react": "^16.9.41",
    "@types/react-dom": "^16.9.8",
    "@types/webpack-env": "^1.15.2",
    "@typescript-eslint/eslint-plugin": "^3.6.0",
    "@typescript-eslint/parser": "^3.6.0",
    "babel-loader": "^8.1.0",
    "css-loader": "^3.6.0",
    "eslint": "^7.4.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.3",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.3.0",
    "prettier": "^2.0.5",
    "source-map-loader": "^1.0.1",
    "style-loader": "^1.2.1",
    "ts-loader": "^7.0.5",
    "typescript": "^3.9.6",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "@apollo/react-hooks": "^3.1.5",
    "antd": "^4.4.1",
    "apollo": "^2.28.3",
    "apollo-boost": "^0.4.9",
    "graphql": "14.0.2 - 14.2.0 || ^14.3.1",
    "graphql-tag": "^2.10.3",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  }
}
